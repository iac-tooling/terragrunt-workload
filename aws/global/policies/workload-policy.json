{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "AllowFullAccessToRoot",
        "Effect": "Allow",
        "Principal": {
          "AWS": "arn:aws:iam::123456789:root"
        },
        "Action": "kms:*",
        "Resource": "*"
      },
      {
        "Sid": "AllowTlgAdminFullAccess",
        "Effect": "Allow",
        "Principal": {
          "AWS": "arn:aws:iam::123456789:user/admin"
        },
        "Action": "kms:*",
        "Resource": "*"
      }
    ]
  }
  